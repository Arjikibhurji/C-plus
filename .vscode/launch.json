{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch Debugger",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Debugger/main",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "build Debugger",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
